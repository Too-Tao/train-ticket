#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_mocker-api@1.7.6@mocker-api/bin/mocker.js" "$@"
  ret=$?
else 
  node  "$basedir/../_mocker-api@1.7.6@mocker-api/bin/mocker.js" "$@"
  ret=$?
fi
exit $ret
